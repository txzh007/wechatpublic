<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="link.tanxin.wechatpublic.model.UserInfoEntity" table="user_info" schema="wechat_dev">
        <id name="id" column="id"/>
        <property name="nickname" column="nickname"/>
        <property name="mobile" column="mobile"/>
        <property name="sex" column="sex"/>
        <property name="openid" column="openid"/>
        <property name="country" column="country"/>
        <property name="language" column="language"/>
        <property name="headimgurl" column="headimgurl"/>
        <property name="modifiedTime" column="modified_time"/>
        <property name="createdTime" column="created_time"/>
        <property name="city" column="city"/>
        <property name="delFlag" column="del_flag"/>
    </class>
</hibernate-mapping>